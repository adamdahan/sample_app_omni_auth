<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= render 'form', :locals => {:name => @user.name, :email => @user.email, :token => @user.token, :facebook_id => @user.facebook_id, :expires => @user.expires, :picture => @user.picture }%>
  </div>
</div>
